{
  "name": "vortex-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "seed": "ts-node --transpile-only prisma/seed.ts",
    "db:reset": "prisma migrate reset --force && prisma generate && ts-node --transpile-only prisma/seed.ts",
    "studio:direct": "powershell -Command \"$env:DATABASE_URL=$env:DIRECT_URL; npx prisma studio\"",
    "postinstall": "prisma generate",
    "prebuild": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.2",
    "@supabase/supabase-js": "^2.57.4",
    "autoprefixer": "10.4.20",
    "bcryptjs": "^2.4.3",
    "clsx": "2.1.1",
    "geoip-lite": "^1.4.10",
    "lucide-react": "^0.542.0",
    "next": "14.2.4",
    "next-auth": "^4.24.11",
    "next-intl": "^4.3.5",
    "postcss": "8.4.41",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^7.1.11",
    "recharts": "^2.12.7",
    "tailwindcss": "3.4.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/geoip-lite": "^1.4.4",
    "@types/node": "20.14.11",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "eslint": "8.57.0",
    "eslint-config-next": "^14.2.4",
    "prisma": "^6.16.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  }
}
