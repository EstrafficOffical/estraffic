# PostgreSQL connection string, e.g.:
# DATABASE_URL="postgresql://postgres:postgres@localhost:5432/vortex?schema=public"
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/vortex?schema=public"

# NextAuth
NEXTAUTH_SECRET="replace_me_with_a_long_random_string"
NEXTAUTH_URL="http://localhost:3000"

# For password hashing (bcrypt salt rounds)
BCRYPT_SALT_ROUNDS="12"

# Tracking security (to validate postbacks)
POSTBACK_SHARED_SECRET="change_me"
